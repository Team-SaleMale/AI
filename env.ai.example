# FastAPI Runtime
UVICORN_WORKERS=2
UVICORN_HOST=0.0.0.0
UVICORN_PORT=8000

# Database Configuration (RDS - Spring Boot와 동일한 DB 사용)
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=your-rds-endpoint.region.rds.amazonaws.com
DB_PORT=5432
DB_NAME=your_database_name

# Redis Configuration (Optional - compose 내부 사용 시)
REDIS_HOST=redis
REDIS_PORT=6379

# Docker Hub Username (for image tagging)
DOCKERHUB_USERNAME=your_dockerhub_username

# Optional: 외부 API 키/엔드포인트가 있다면 아래에 추가
# EXTERNAL_API_URL=
# EXTERNAL_API_KEY=


